{
  "name": "overview-amenities",
  "version": "1.0.0",
  "description": "The Overview, Amenities, and sticky nav bar module for overview FEC",
  "main": "./server/index.js",
  "scripts": {
    "build": "webpack -w",
    "start": "nodemon server/index.js",
    "createjsonM": "node db/mongo/createjson.js",
    "seedM": "mongoimport --db amenities-overview --collection listingdetails --type json --file db/mongo/10M.json --numInsertionWorkers 8",
    "createjsonP": "node db/postgres/createjson.js",
    "createcsvP": "json2csv -i db/postgres/10M.json -f listing_ID,propertyType,overview,amenities,houseRules,tags -o db/postgres/10M.csv",
    "createschemaP": "psql -f db/postgres/schema.sql",
    "seedP": "psql -f db/postgres/seedPg.sql",
    "createjsonP:createcsvP:createschemaP:seedP": "npm run createjsonP && npm run createcsvP && npm run createschemaP && npm run seedP"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrla33-falcons/overview-amenities.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hrla33-falcons/overview-amenities/issues"
  },
  "homepage": "https://github.com/hrla33-falcons/overview-amenities#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cli-progress": "^3.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "json2csv": "^4.5.4",
    "mongodb": "^2.2.33",
    "mongoose": "^5.7.13",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.1",
    "path": "^0.12.7",
    "pg": "^7.15.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "zipcodes": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
